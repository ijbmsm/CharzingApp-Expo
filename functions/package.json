    "cors": "^2.8.5",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1",
    "googleapis": "^164.1.0",
    "puppeteer-core": "^24.34.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/markdown-it": "^14.1.2",
    "markdown-it": "^14.1.0",
    "typescript": "^4.9.0"
  },
