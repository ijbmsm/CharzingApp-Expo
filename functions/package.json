{
  "name": "charzing-cloud-functions",
  "version": "1.0.0",
  "description": "CharzingApp Cloud Functions for production backend",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "expo-web-browser": "^15.0.7",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1",
    "googleapis": "^161.0.0",
    "expo": "~54.0.10",
    "react": "19.1.0",
    "react-native": "0.81.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "typescript": "^4.9.0"
  },
  "private": true
}
