{
  "name": "charzingapp-expo",
  "main": "App.tsx",
  "version": "1.1.8",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "typecheck": "tsc --noEmit",
    "upload:vehicles": "ts-node scripts/uploadVehiclesToFirebase.ts",
    "test:transform": "node scripts/runUpload.js",
    "firebase:commands": "node scripts/uploadHyundaiKiaToFirebase.js",
    "upload:simple": "node scripts/uploadVehiclesSimple.js",
    "build:android": "./scripts/build-android.sh",
    "build:android:apk": "./scripts/build-android.sh release apk",
    "build:android:clean": "./scripts/build-android.sh release aab --clean",
    "build:ios": "./scripts/build-ios.sh",
    "build:ios:clean": "./scripts/build-ios.sh --clean",
    "build:eas:android": "eas build --platform android --profile production",
    "build:eas:ios": "eas build --platform ios --profile production",
    "build:eas:all": "eas build --platform all --profile production",
    "env:pull": "eas env:pull --environment production --path .env.production",
    "sync-versions": "node scripts/sync-versions.js",
    "version:patch": "npm version patch && npm run sync-versions",
    "version:minor": "npm version minor && npm run sync-versions",
    "version:major": "npm version major && npm run sync-versions",
    "postversion": "npm run sync-versions && git add . && git commit --amend --no-edit"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
